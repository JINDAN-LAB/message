<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jindan.jdy.common.mapper.JdyPutTypeMapper">

	<resultMap id="BaseResultMap" type="com.jindan.jdy.common.pojo.JdyPutType">
		<id column="put_type_id" property="putTypeId" />
		<id column="put_type_name" property="putTypeName" />
		<id column="put_type_type" property="putTypeType" />
		<id column="parent_id" property="parentId" />
		<id column="insert_time" property="insertTime" />
		<id column="update_time" property="updateTime" />
		<id column="delete_id" property="deleteId" />
	</resultMap>

	<resultMap id="BaseDtosMap" type="com.jindan.jdy.common.dto.JdyPutTypeDto">
		<id column="put_type_id" property="putTypeId" />
		<id column="put_type_name" property="putTypeName" />
		<id column="put_type_type" property="putTypeType" />
		<id column="parent_id" property="parentId" />
		<id column="insert_time" property="insertTime" />
		<id column="update_time" property="updateTime" />
		<id column="delete_id" property="deleteId" />
		<id column="orders" property="orders" />
	</resultMap>

	<sql id="Base_Column_List">
		put_type_id, put_type_name, put_type_type, parent_id, insert_time, update_time, delete_id
	</sql>

<!--	selectAll-->
	<select id="selectAll"   resultMap="BaseDtosMap">
		SELECT  put_type_id,put_type_name,put_type_type,parent_id,insert_time,update_time,delete_id,orders
		FROM jdy_put_type   where  jdy_put_type.delete_id =0
	</select>


</mapper>