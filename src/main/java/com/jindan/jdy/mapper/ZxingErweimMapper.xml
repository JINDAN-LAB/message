<!--	seletelistseleteOne-->
	<select id="seletelistseleteOne"  parameterType="java.lang.String" resultMap="CatalogueDto">
		select
		 zxing_erweim.*,
		 zxing_method.id as mid,
		 zxing_method.methodurl as mmethodurl,
		 zxing_stand.standurl as sstandurl,
		 zxing_stand.id as sid
		from zxing_erweim
		left join zxing_method  on zxing_method.method = zxing_erweim.method
		left join zxing_stand on zxing_stand.standard = zxing_erweim.standard
		where zxing_erweim.md=#{id}
	</select>

